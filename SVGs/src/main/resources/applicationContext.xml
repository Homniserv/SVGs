<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
	http://www.springframework.org/schema/context
	http://www.springframework.org/schema/context/spring-context-2.5.xsd">

	<context:annotation-config />
	<context:component-scan
		base-package="presentation" />
	<bean id="configurationBean"
		class="presentation.ConfigurationBean">


		<property name="interPerson" value="20" />


		<property name="interPool" value="40" />

		<property name="widthSheet" value="1000" />
		<property name="heightSheet" value="1000" />
		<property name="lineLength" value="30" />
		<property name="fontSize" value="13" />
		<property name="personRelativePoolX" value="5" />
		<property name="poolX" value="10" />
		<property name="poolY" value="10" />
		<property name="marginNameX" value="8" />

	</bean>

</beans>